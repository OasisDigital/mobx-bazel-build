{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "downlevelIteration": true,
        "removeComments": false,
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "experimentalDecorators": true,
        "lib": ["es6"],
        "skipLibCheck": true,
        "types": ["node"]
    }
}
